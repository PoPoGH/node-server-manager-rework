<html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=0.7">
        <title>Server Overview | Node Server Manager</title>
        <meta property="og:title" content="Server Overview | Node Server Manager">
        <meta property="og:type" content="website">
        <meta property="og:description" content="Node Server Manager is an administration tool focused on Dedicated Servers">
        <meta name="description" content="Node Server Manager is an administration tool focused on Dedicated Servers">
        <meta name="keywords" content="IW4MAdmin, IW5, Plutonium, T6, NodeJs, server, administration, IW4x, MW2, Modern Warfare 3">
        <script type='text/javascript' src='/js/audit.js'></script>
        <script type='text/javascript' src='/js/main.js'></script>
        <link rel="stylesheet" type="text/css" href="/css/xbbcode.css">
        <script src="/js/xbbcode.js" type="text/javascript"></script>
        <script type='text/javascript' src='/js/notifications.js'></script>
        <script type='text/javascript' src='/js/font-awesome.js'></script>
        <script type='text/javascript' src='/js/moment-with-locales.js'></script>
        <script type='text/javascript' src='/js/moment-timezone-with-data-10-year-range.js'></script>
        <link rel='stylesheet' type='text/css' href='css/global.css'></link>
    </head>
    <body class='nice-scrollbar'>
        <%- header%>
        <div class='wf-client-search'>
            <div class='wf-profile wf-default'>
            <div class='wf-client-search-list' id='audit-cont'>
                <div class='wf-table-row'>
                    <div>Origin</div>
                    <div>Action</div>
                    <div>Description</div>
                    <div>Date</div>
                </div>
                <% Audit.forEach(log => { %>
                    <% if (!log) return %>
                    <div class='wf-table-row' >
                        <% if (log.Origin.ClientId) {%>
                            <a class='wf-link' href='/id/<%=log.Origin.ClientId%>'><%= log.Origin.Name %></a>
                        <% } else {%>
                            <div><%= log.Origin.Name %></div>
                        <%}%>
                        <div><%= log.Type %></div>
                        <div><%= log.Description  %></div>
                        <div date-moment><%= log.Date.toISOString()  %></div>
                    </div>
                <% }) %>
            </div>
        </div>
        </div>
    </body>
</html>