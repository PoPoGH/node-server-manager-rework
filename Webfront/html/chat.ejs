<html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=0.7">
        <title>Server Overview | Node Server Manager</title>
        <meta property="og:title" content="Server Overview | Node Server Manager">
        <meta property="og:type" content="website">
        <meta property="og:description" content="Node Server Manager is an administration tool focused on Dedicated Servers">
        <meta name="description" content="Node Server Manager is an administration tool focused on Dedicated Servers">
        <meta name="keywords" content="IW4MAdmin, IW5, Plutonium, T6, NodeJs, server, administration, IW4x, MW2, Modern Warfare 3">
        <script type='text/javascript' src='/js/main.js'></script>
        <script type='text/javascript' src='/js/chat.js'></script>
        <script src="/js/moment-with-locales.js" type="text/javascript"></script>
        <link rel="stylesheet" type="text/css" href="/css/xbbcode.css">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/flag-icon-css/3.5.0/css/flag-icon.min.css">
        <script src="/js/xbbcode.js" type="text/javascript"></script>
        <script src="/js/moment-timezone-with-data-10-year-range.js" type="text/javascript"></script>
        <script type='text/javascript' src='/js/font-awesome.js'></script>
        <link rel='stylesheet' type='text/css' href='/css/global.css'></link>
    </head>
    <body>
        <%- header %>
            <div class='wf-chatbox nice-scrollbar' id='message-log'>
                <%  var previousDate = new Date()
                    var i = 0; Messages.forEach(Message => { %>
                        <% if (i++ < Messages.length - 5) { %>
                            <% if ( ( previousDate - new Date(Message.Date) ) / 1000 / 60 / 30 >= 1) { 
                                    previousDate = new Date(Message.Date) %>
                                <div class='wf-chat-separator-wrap'>
                                    <div class='wf-chat-separator'>
                                        <span date-moment><%= Message.Date %></span>
                                    </div>
                                </div>
                    <% } } %>
                            <div class='wf-message'>
                                <div class='wf-message-sender'>
                                    <a data-profile-preview data-clientid='<%= Message.OriginId %>' class='wf-link wf-message-sender' href='/id/<%= Message.OriginId %>' colorcode><%= Message.Name %></a> @ <span class='api-data-type' colorcode> <%=Message.Hostname%> </span>:</div>
                                <div class='wf-message-message'><%= Message.Message %></div>
                                <div class='wf-default wf-message-date' date-moment><%= Message.Date %></div>
                            </div>
                <% }) %>%>
        </div>
    </body>
</html>