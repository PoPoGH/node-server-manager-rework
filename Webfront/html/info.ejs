<html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=0.7">
        <title>Info | Node Server Manager</title>
        <meta property="og:title" content="Info | Node Server Manager">
        <meta property="og:type" content="website">
        <meta property="og:description" content="Node Server Manager is an administration tool focused on Dedicated Servers">
        <meta name="description" content="Node Server Manager is an administration tool focused on Dedicated Servers">
        <meta name="keywords" content="IW4MAdmin, IW5, Plutonium, T6, NodeJs, server, administration, IW4x, MW2, Modern Warfare 3">
        <script type='text/javascript' src='/js/main.js'></script>
        <script type='text/javascript' src='/js/info.js'></script>
        <link rel="stylesheet" type="text/css" href="/css/xbbcode.css">
        <script src="/js/xbbcode.js" type="text/javascript"></script>
        <script src="/js/moment-with-locales.js" type="text/javascript"></script>
        <script src="/js/moment-timezone-with-data-10-year-range.js" type="text/javascript"></script>
        <script type='text/javascript' src='/js/font-awesome.js'></script>
        <link rel='stylesheet' type='text/css' href='/css/global.css'></link>
    </head>
    <body>
        <%- header %>
        <div class='wf-info wf-default'>
            <div class='wf-profile-description-wrap'>
                <div class='wf-profile-description-line'>
                    <pre class="wf-info-text"  style='display:none' data-textbox data-xbbcode data-raw-text="<%= Info %>">
                        <%= Info %>
                    </pre>
                </div>
                <% if ( Client && Client.PermissionLevel >= Permissions.Levels[Permissions.Commands.COMMAND_CHANGE_INFO] ) {%>
                <div class='wf-profile-description-button' onClick='infoEdit()' data-profile-submit style='display:block'>
                    Edit
                </div>
                <%}%>
            </div>
        </div>
    </body>
</html>