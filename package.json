{
  "name": "node-server-manager",
  "version": "1.0.0",
  "description": "Gestionnaire de serveurs Plutonium modernisé et restructuré",
  "main": "src/core/index.js",
  "scripts": {
    "start": "node src/core/index.js",
    "start:api": "node src/core/api-server.js",
    "start:logs": "node src/core/log-server.js",
    "start:minimal": "node src/core/minimal-server.js",
    "dev": "nodemon src/core/index.js",
    "dev:api": "nodemon src/core/api-server.js",
    "dev:web": "cd src/web && npm run dev",
    "build:web": "cd src/web && npm run build",
    "install:web": "cd src/web && npm install",
    "backup": "node scripts/maintenance/backup-db.js",
    "restore": "node scripts/maintenance/restore-db.js",
    "diagnostic": "node scripts/diagnostic/run-diagnostics.js",
    "test": "jest",
    "setup": "npm install && npm run install:web"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/votrenom/node-server-manager.git"
  },
  "keywords": [
    "plutonium",
    "cod",
    "server-manager",
    "rcon"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "delay": "^4.4.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^3.0.5",
    "ip-range-check": "^0.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.7",
    "tail": "^2.2.6",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "axios": "^1.9.0",
    "colors": "^1.4.0",
    "debug": "^4.4.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  }
}
